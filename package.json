{
  "name": "stack-converter-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered codebase converter - monorepo",
  "scripts": {
    "dev": "pnpm --parallel --recursive run dev",
    "dev:frontend": "pnpm --filter frontend dev",
    "dev:backend": "pnpm --filter backend dev",
    "start:backend": "pnpm --filter backend start",
    "build:frontend": "pnpm --filter frontend build",
    "preview:frontend": "pnpm --filter frontend preview",
    "clean": "pnpm --recursive run clean",
    "lint": "pnpm --recursive run lint",
    "test": "pnpm --filter backend test",
    "test:watch": "pnpm --filter backend test:watch",
    "test:coverage": "pnpm --filter backend test:coverage"
  },
  "keywords": ["monorepo", "ai", "code-converter", "stack-converter"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
} 